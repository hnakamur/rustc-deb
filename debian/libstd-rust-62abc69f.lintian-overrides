# "libstd" just seemed too generic
libstd-rust-62abc69f binary: package-name-doesnt-match-sonames

# Rust doesn't use dev shlib symlinks.
libstd-rust-62abc69f binary: dev-pkg-without-shlib-symlink

# Libraries that use libc symbols (libterm, libstd, etc) *are* linked
# to libc.  Lintian gets upset that some Rust libraries don't need
# libc, boo hoo.
libstd-rust-62abc69f binary: library-not-linked-against-libc
